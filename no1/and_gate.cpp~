#include<stdio.h>
#include <iostream>
#include <vector>
#include <numeric>

int and(x1,x2){
  std::vector<int> x;
  x.push_back(x1);
  x.push_back(x2);

  std::vector<int> y = {0.5, 0.5};
  b = -0.7;

  int tmp = std::inner_product( x.begin(), x.end(), y.begin(), 0) + b;
  if( tmp > 0){
    return  1;
  }else{
    return -1;
  }
}

int main (void){
  std::cout << "hello world" << std::endl;
  int xs = {{0,0},{1,0},{0,1},{1,1}};
  for(int i = 0; i < 3; i++){
    std::cout << xs[i][0] << ":" << xs[i][1] << "->" and(xs[i][0],xs[i][1]) << std::endl;
  }
  return 0;
}
